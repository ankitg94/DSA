#include <iostream>
using namespace std;
int main() {
    int arr[10]={9,1,2,5,6,7};
    
    
    for(int i=0;i<=10;i++){
        for(int j=i+1;j<10;j++){
         if(arr[i]>arr[j]){
             swap(arr[i],arr[j]);
         }
            
        }
    }
    for(int i=0;i<10;i++){
        cout<<arr[i];
    }

    
//insertion sort
int arr[100];
int n;
cout<<"Enter the length of arr: ";
cin>>n;
for(int i=0;i<n;i++){
    cin>>arr[i];
}
for(int i=1;i<n;i++){
    for(int j=i;j>0;j--){
             if(arr[j]<arr[j-1])
            swap(arr[j],arr[j-1]);
    }
}

for(int i=0;i<n;i++){
    cout<<arr[i]<<" ";
}
  

    return 0;
}
