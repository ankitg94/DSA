// Online C++ compiler to run C++ program online 
#include <iostream> 
using namespace std; 
int main() { 
    int n; 
    cout<<"Enter the number : "; 
    cin>>n; 
 
    // square box 
    cout<<endl; 
    cout<<"Q1-Square box : "<<endl; 
    cout<<endl; 
     
    for(int i=0;i<n;i++){ 
        for(int j=0;j<n;j++){ 
             cout<<"*"; 
            //cout<<i; 
            //cout<<j; 
        } 
        cout<<endl; 
    } 
     
    //sum of number; 
    cout<<endl; 
    cout<<"Q 2- sum of num :"<<endl; 
    cout<<endl; 
     
    int num=1; 
    for(int i=0;i<n;i++){ 
         for(int j=0;j<n;j++){ 
             //  cout<<"*"; 
          cout<<" "<<num; 
            num++; 
    } 
         cout<<endl; 
    } 
     
     
    //trianlge; 
    cout<<endl; 
    cout<<"Q3-triangle : "<<endl; 
    cout<<endl; 
     
    for(int i=1;i<=n;i++){ 
        for(int j=1;j<i+1;j++){ 
            //cout<<"*"; 
            //cout<<i; 
            cout<<j; 
        } 
        cout<<endl; 
    } 
     
    //reverse of triangle 
    cout<<endl; 
    cout<<"Q4-reverse of triangle : "; 
    cout<<endl; 
     
    for(int i=0;i<n;i++){ 
        for(int j=i+1;j>=1;j--){ 
            cout<<j<<" "; 
        } 
        cout<<endl; 
    } 
     
    //Floyd triangle 
    cout<<endl; 
    cout<<"Q5-Floyd Triangle :"; 
    cout<<endl; 
    //int num=1; 
    for(int i=1;i<=n;i++){ 
        for(int j=1;j<i+1;j++){ 
            cout<<" "<<num; 
            num++; 
        } 
        cout<<endl; 
    } 
     
    //space +triangle 
    cout<<endl; 
    cout<<"Q6 - space + triangle"; 
    cout<<endl; 
     
    for(int i=1;i<n;i++){ 
        for(int j=1;j<=i+1;j++){ 
            cout<<"  "; 
        } 
        for(int j=i;j<n;j++){ 
            cout<<i<<" "; 
        } 
        cout<<endl; 
    } 
     
    //numerical pyramid inc +dec   
    cout<<endl; 
    cout<<"Q7 - numerical pyramid inc +dec "; 
    cout<<endl; 
    for(int i=1;i<=n;i++){ 
        for(int j=n;j>=i;j--){ 
            cout<<"  "; 
        } 
        for(int j=1;j<i+1;j++){ 
            cout<<j<<" "; 
        } 
        for(int j=i-1;j>=1;j--){ 
            cout<<j<<" "; 
        } 
        cout<<endl; 
    } 
    //pyramid 
    cout<<endl; 
    cout<<"Q 8- pyramid"; 
    cout<<endl; 
    for( int i=1;i<n;i++){ 
        for(int j=i;j<n;j++){ 
            cout<<" "; 
        } 
        for(int j=1;j<i+1;j++){ 
            cout<<" *"; 
        } 
        cout<<endl; 
    } 
    //reverse pyramid 
    cout<<endl; 
    cout<<"Q9- reverse Pyramid "; 
    cout<<endl; 
    for(int i=1;i<=n;i++){ 
        for(int j=1;j<i+1;j++){ 
        cout<<" ";     
        } 
         for(int j=i;j<n;j++){ 
             cout<<" *"; 
         } 
        cout<<endl; 
    } 
     
    cout<<endl; 
    cout<<"Q10 -dimond or rhombus"; 
    cout<<endl; 
    for(int i=1;i<n;i++){ 
        for(int j=i;j<n;j++){ 
            cout<<" "; 
        } 
        for(int j=1;j<i+1;j++){ 
            cout<<" *"; 
        } 
        cout<<endl; 
    } 
    for(int i=1;i<n;i++){ 
     for(int j=1;j<i+1;j++){ 
         cout<<" "; 
     } 
      for(int j=i;j<n;j++){ 
          cout<<" *"; 
      } 
      cout<<endl; 
    }     
     
     
     
     
  //hollow rohmbus 
  cout<<endl; 
  cout<<"Q11- hollow pyramid"<<endl; 
  cout<<endl; 
    
  for(int i=1;i<n;i++){ 
      for(int j=i;j<=n;j++){ 
          cout<<" "; 
      } 
      cout<<"  *"; 
      if(i!=1){ 
          for(int j=2;j<i*2;j++){ 
              cout<<" "; 
          } 
        cout<<"*"; 
      } 
      cout<<endl; 
  } 
  for(int i=1;i<n;i++){ 
      for (int j=1;j<=i+1;j++){ 
          cout<<" "; 
      } 
      cout<<" *"; 
      if(i!=n-1){ 
         for(int j=1;j<=2*(n-i-1);j++){ 
             cout<<" "; 
         }   
         cout<<"*"; 
      } 
      cout<<endl; 
  } 
  //butterfly 
  cout<<endl; 
  cout<<"Q12 - butterfly"<<endl; 
  cout<<endl; 
    
  for(int i=1;i<n;i++){ 
  for(int j=1;j<i+1;j++){ 
      cout<<" *"; 
  }  
  for(int j=1;j<=2*(n-i-1);j++){ 
      cout<<"  "; 
  } 
  for(int j=1;j<i+1;j++){ 
      cout<<" *"; 
  } 
  cout<<endl; 
}    
  for(int i=1;i<n;i++){ 
      for(int j=i;j< n;j++){ 
          cout<<" *"; 
      } 
      for(int j=1;j<=2*(i-1);j++){ 
          cout<<"  "; 
      } 
      for(int j=i;j<n;j++){ 
          cout<<" *"; 
      } 
       
      cout<<endl; 
  } 
  //hour glass  
  cout<<endl; 
  cout<<"Q13- Hour glass "<<endl; 
  cout<<endl; 
  for(int i=0;i<=n;i++){ 
      cout<<"* "; 
  } 
  cout<<endl; 
  for(int i=0;i<n; i++){ 
     if(i!=n){ 
      for(int j=1;j<i+1;j++){ 
        cout<<" "; 
      } 
      cout<<"*"; 
     } 
     if(i!=n-1){ 
     for(int j=i;j<(2*(n-1))-i;j++){ 
         cout<<" "; 
     } 
     cout<<"*"; 
     } 
      cout<<endl; 
  } 
  for(int i=0;i<n;i++){ 
      for(int j=i;j<n;j++){ 
          cout<<" "; 
      } 
      cout<<"*"; 
      if(i!=0){ 
      for(int j=1;j<2*i;j++){ 
          cout<<" "; 
      } 
      cout<<"*"; 
      } 
      cout<<endl; 
  } 
  for(int i=0;i<=n;i++){ 
      cout<<"* "; 
  } 
  cout<<endl; 
  // hollow triangle 
  cout<<endl; 
  cout<<"Q14- Hollow triangle"; 
  cout<<endl; 
   
   
  int number=1;
  for(int i=1;i<=n;i++){ 
      for(int j=i;j<n;j++){ 
          cout<<" "; 
      } 
      for(int j=1;j<=2*i-1;j++){
          if(j==1 || j==2*i-1){
              cout<<number<<" ";
              number++;
          }
          else{
              cout<<" ";
          }
      }
      cout<<endl;
  } 
  for(int i=1;i<n;i++){
      cout<<number<<" ";
      number++;
  }
  
  //alphabet series 
  cout<<endl;
  cout<<"Q15- Alphabet -A"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
     
      for(int j=1;j<=n;j++){
      if(i==1 || i==(n/2+1) ||j==1 || j==n){
          cout<<"*";
      }
      else{
          cout<<" ";
      }
      }
       cout<<endl;
  }
  
  cout<<endl;
  cout<<"Alphabet -B"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
     
      for(int j=1;j<=n;j++){
      if(i==1 || i==(n/2+1) ||i==n ||j==1 || j==n){
          cout<<"*";
      }
      else{
          cout<<" ";
      }
      }
       cout<<endl;
  }
  cout<<endl;
  cout<<"Alphabet -c"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
     
      for(int j=1;j<=n;j++){
      if(i==1  ||i==n ||j==1 ){
          cout<<"*";
      }
      else{
          cout<<" ";
      }
      }
       cout<<endl;
  }
  
  cout<<endl;
  cout<<"Alphabet-D"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
     
      for(int j=1;j<=n;j++){
      if(i==1  ||i==n ||j==1 || j==n){
          cout<<"*";
      }
      else{
          cout<<" ";
      }
      }
       cout<<endl;
  }
  
  
  cout<<endl;
  cout<<"Alphabet -E"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
     cout<<"*";
      for(int j=1;j<=n;j++){
      if(i==1 || i==n || i==(n/2)+1){
          cout<<"*";
      }
       }
      cout<<endl;
  }
   
 
 
  cout<<endl;
  cout<<"Alphabet-f"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
     cout<<"*";
      for(int j=1;j<=n;j++){
      if(i==1  || i==(n/2)+1){
          cout<<"*";
      }
       }
      cout<<endl;
  }
   
  cout<<endl;
  cout<<"Alphabet-H"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
      for(int j=1;j<=n;j++){
      if(i==(n/2)+1||j==1 || j==n){
          cout<<"*";
      }
      else{
          cout<<" ";
      }
       }
      cout<<endl;
  }
   
   cout<<endl;
  cout<<"Alphabet-I"<<endl;
  cout<<endl;
  
  for(int i=1;i<=n;i++){
      for(int j=1;j<=n;j++){
      if(i==1||i==n || j==n/2){
          cout<<"*";
      }
      else{
          cout<<" ";
      }
       }
      cout<<endl;
  }
    
    
     cout<<endl;
  
   
   
   
    return 0; 
} 
