#include <iostream>
using namespace std;

int binarySearch(int arr[], int n,int key ){
 int start=0;
 int end=n;
 while(start < end){
    int  mid=(start+end)/2;
     if(arr[mid] == key){
         return mid;
     }
     else if(arr[mid]> key){
         end = mid-1;
     }
     else{
         start = mid+1;
     }
 }
 return -1;
}
int main() {

 int arr[10]= {1,2,3,7,8,9,12,19,25,27};
 int n =sizeof(arr)/sizeof(int);
 cout<<"size"<<n<<endl;
 
 int key;
 cout<<"Enter the key  : ";
 cin>>key;
 cout<<binarySearch(arr , n ,key);
 
    return 0;
}
