#include <iostream>
using namespace std;
int main() {
    int arr[10]={9,1,2,5,6,7};
    
    
    for(int i=0;i<=10;i++){
        for(int j=i+1;j<10;j++){
         if(arr[i]>arr[j]){
             swap(arr[i],arr[j]);
         }
            
        }
    }
    for(int i=0;i<10;i++){
        cout<<arr[i];
    }
  

    return 0;
}
